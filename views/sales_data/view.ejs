<%- include ../partials/header.ejs %>
<%- include ../partials/sidebar.ejs %>
<%- include ../partials/navbar.ejs %>
<%- include ../partials/flash.ejs %>


<!-- Basic Form Start -->
<div class="basic-form-area mg-b-15">
    <div class="container-fluid">


        <div class="row">
            <div class="col-lg-12">
                <div class="sparkline12-list shadow-reset mg-t-30">
                    <div class="sparkline12-hd">
                        <div class="main-sparkline12-hd">
                            <h1>View Order</h1>
                            <!-- <div class="sparkline12-outline-icon">
                                <span class="sparkline12-collapse-link"><i class="fa fa-chevron-up"></i></span>
                                <span><i class="fa fa-wrench"></i></span>
                                <span class="sparkline12-collapse-close"><i class="fa fa-times"></i></span>
                            </div> -->
                        </div>
                    </div>
                    <div class="sparkline12-graph">
                        <div class="basic-login-form-ad">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="all-form-element-inner">
                                        <form action="/update_order" class="adminpro-form" method="post" encType="multipart/form-data">
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <input type="hidden" id="check_id" value="0">
                                                        <input type="hidden" id="check_model" value="User">

                                                        <label class="login2 pull-right pull-right-pro">Vanue</label>
                                                    </div>
                                                    <% if(response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.venue && (response.dataValues.venue!='' || response.dataValues.venue!='null') && response.dataValues.venue.dataValues.name && response.dataValues.venue.dataValues.name){%>
                                                    <div class="col-lg-9 span_text">

                                                        <span>
                                                            <%= (response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.venue && (response.dataValues.venue!='' || response.dataValues.venue!='null') && response.dataValues.venue.dataValues.name && response.dataValues.venue.dataValues.name) ?response.dataValues.venue.dataValues.name :''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Username</label>
                                                    </div>
                                                    <% if(response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.user && (response.dataValues.user!='' || response.dataValues.user!='null') && response.dataValues.user.dataValues.name && response.dataValues.user.dataValues.name){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= (response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.user && (response.dataValues.user!='' || response.dataValues.user!='null') && response.dataValues.user.dataValues.name && response.dataValues.user.dataValues.name) ?response.dataValues.user.dataValues.name :''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Total</label>
                                                    </div>
                                                    <% if(response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.total && (response.dataValues.total!='' || response.dataValues.total!='null')){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= (response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.total && (response.dataValues.total!='' || response.dataValues.total!='null')) ? response.dataValues.total:''%>
                                                        </span>
                                                    </div>
                                                    <% }%>
                                                </div>
                                            </div>

                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Round</label>
                                                    </div>
                                                    <% if(response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.round && (response.dataValues.round!='' || response.dataValues.round!='null')){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= (response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.round && (response.dataValues.round!='' || response.dataValues.round!='null')) ? response.dataValues.round:''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Service</label>
                                                    </div>
                                                    <% if(response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.tbmService && (response.dataValues.tbmService!='' || response.dataValues.tbmService!='null')){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= (response && response !='' && response.dataValues && response.dataValues!='' && response.dataValues.tbmService && (response.dataValues.tbmService!='' || response.dataValues.tbmService!='null')) ? response.dataValues.tbmService :''%>
                                                        </span>
                                                    </div>
                                                    <% }%>
                                                </div>
                                            </div>






                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Basic Form End-->


<% include ../partials/footer.ejs %>