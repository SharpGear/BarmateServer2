<%- include ../partials/header.ejs %>
    <%- include ../partials/sidebar.ejs %>
        <%- include ../partials/navbar.ejs %>
            <%- include ../partials/flash.ejs %>



                <!-- Basic Form Start -->
                <div class="basic-form-area mg-b-15">
                    <div class="container-fluid">


                        <div class="row">
                            <div class="col-lg-12">
                                <div class="sparkline12-list shadow-reset mg-t-30">
                                    <div class="sparkline12-hd">
                                        <div class="main-sparkline12-hd">
                                            <h1>Edit Terminal</h1>
                                            <!-- <div class="sparkline12-outline-icon">
                                <span class="sparkline12-collapse-link"><i class="fa fa-chevron-up"></i></span>
                                <span><i class="fa fa-wrench"></i></span>
                                <span class="sparkline12-collapse-close"><i class="fa fa-times"></i></span>
                            </div> -->
                                        </div>
                                    </div>
                                    <div class="sparkline12-graph">
                                        <div class="basic-login-form-ad">
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="all-form-element-inner">
                                                        <form action="/update_terminal" class="adminpro-form" method="post" encType="multipart/form-data">
                                                            <input type="hidden" name="id" value="<%=result.dataValues.id%>">
                                                            <div class="form-group-inner">
                                                                <div class="row">
                                                                    <div class="col-lg-3">
                                                                        <input type="hidden" id="check_id" value="0">
                                                                        <input type="hidden" id="check_model" value="User">

                                                                        <label class="login2 pull-right pull-right-pro">Venue</label>
                                                                    </div>
                                                                    <div class="col-lg-9">
                                                                        <select class="form-control" id="venue_id" name="venue" required>
                                  <option value="" disabled selected>Select venue </option>
                                  <% 
                                                  if(response && response.length>0){
                                                      for(var i in response){
    
                                                               %>
                                  <option value="<%= response[i].id? response[i].id : '' %>"
                                        <% if(response[i].id==result.dataValues.venue_id){%> selected <%}%>>
                                    <%= response[i].name? response[i].name : '' %>
                                  </option>
                                  <% }
                                                    }%>
                                </select>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="form-group-inner">
                                                                <div class="row">
                                                                    <div class="col-lg-3">
                                                                        <input type="hidden" id="check_id" value="0">
                                                                        <input type="hidden" id="check_model" value="User">

                                                                        <label class="login2 pull-right pull-right-pro">Name</label>
                                                                    </div>
                                                                    <div class="col-lg-9">
                                                                        <input type="text" class="form-control" placeholder="Terminal Name" name="name" value="<%=result.dataValues.name%>" required />
                                                                    </div>
                                                                </div>
                                                            </div>

                                                            <div class="form-group-inner">
                                                                <div class="row">
                                                                    <div class="col-lg-3">
                                                                        <label class="login2 pull-right pull-right-pro">Email</label>
                                                                    </div>
                                                                    <div class="col-lg-9">
                                                                        <input type="email" class="form-control user_email" name="email" value="  <% if(term_email && term_email!='') { %> <%= term_email.dataValues.email ? term_email.dataValues.email: ''%> 
                                                                        <%}%>" placeholder="Terminal Email" required id="name" />
                                                                        <span class="user_email_chk" style="color:red;display:none;">Email already exists</span>

                                                                    </div>
                                                                </div>
                                                            </div>

                                                            <div class="form-group-inner">
                                                                <div class="row">
                                                                    <div class="col-lg-3">
                                                                        <input type="hidden" id="check_id" value="0">
                                                                        <input type="hidden" id="check_model" value="User">

                                                                        <label class="login2 pull-right pull-right-pro">Password</label>
                                                                    </div>
                                                                    <div class="col-lg-9">
                                                                        <input type="password" class="form-control" placeholder="Terminal Password" name="password" required />
                                                                    </div>
                                                                </div>
                                                            </div>







                                                            <div class="form-group-inner">
                                                                <div class="login-btn-inner">
                                                                    <div class="row">
                                                                        <div class="col-lg-3"></div>
                                                                        <div class="col-lg-9">
                                                                            <div class="login-horizental cancel-wp pull-left">
                                                                                <button class="btn btn-white" type="reset"><a href="/terminal"> Cancel</a></button>
                                                                                <button class="btn btn-sm btn-primary login-submit-cs ad_button" type="submit">Save
                                  Change</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Basic Form End-->


                <% include ../partials/footer.ejs %>