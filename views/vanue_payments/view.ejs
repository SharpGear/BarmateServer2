<%- include ../partials/header.ejs %>
<%- include ../partials/sidebar.ejs %>
<%- include ../partials/navbar.ejs %>
<%- include ../partials/flash.ejs %>


<!-- Basic Form Start -->
<div class="basic-form-area mg-b-15">
    <div class="container-fluid">


        <div class="row">
            <div class="col-lg-12">
                <div class="sparkline12-list shadow-reset mg-t-30">
                    <div class="sparkline12-hd">
                        <div class="main-sparkline12-hd">
                            <h1>View Venue Payments</h1>
                            <!-- <div class="sparkline12-outline-icon">
                                <span class="sparkline12-collapse-link"><i class="fa fa-chevron-up"></i></span>
                                <span><i class="fa fa-wrench"></i></span>
                                <span class="sparkline12-collapse-close"><i class="fa fa-times"></i></span>
                            </div> -->
                        </div>
                    </div>
                    <div class="sparkline12-graph">
                        <div class="basic-login-form-ad">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="all-form-element-inner">
                                        <form action="/update_order" class="adminpro-form" method="post" encType="multipart/form-data">
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <input type="hidden" id="check_id" value="0">
                                                        <input type="hidden" id="check_model" value="User">

                                                        <label class="login2 pull-right pull-right-pro">Card Holder
                                                            Name
                                                        </label>
                                                    </div>
                                                    <% if(response.dataValues.cardHolderName && response.dataValues.cardHolderName!=''){%>
                                                    <div class="col-lg-9 span_text">

                                                        <span>
                                                            <%= response.dataValues.cardHolderName ? response.dataValues.cardHolderName:''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Username</label>
                                                    </div>
                                                    <% if(response.dataValues.venue && (response.dataValues.venue!='' || response.dataValues.venue!='null') &&response.dataValues.venue.dataValues.name && response.dataValues.venue.dataValues.name!=''){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= (response.dataValues.venue && (response.dataValues.venue!='' || response.dataValues.venue!='null') &&response.dataValues.venue.dataValues.name && response.dataValues.venue.dataValues.name!='') ? response.dataValues.venue.dataValues.name:''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Card Type</label>
                                                    </div>
                                                    <% if(response.dataValues.cardType && response.dataValues.cardType!=''){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= response.dataValues.cardType ? response.dataValues.cardType:''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>

                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Card Expiry
                                                            Date</label>
                                                    </div>
                                                    <% if(response.dataValues.cardExpiry && response.dataValues.cardExpiry!=''){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= response.dataValues.cardExpiry ? response.dataValues.cardExpiry:''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        <label class="login2 pull-right pull-right-pro">Transaction Id</label>
                                                    </div>
                                                    <% if(response.dataValues.transactionId && response.dataValues.transactionId!=''){%>
                                                    <div class="col-lg-9 span_text">
                                                        <span>
                                                            <%= response.dataValues.transactionId ? response.dataValues.transactionId:''%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                                </div>
                                            </div>






                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Basic Form End-->


<% include ../partials/footer.ejs %>